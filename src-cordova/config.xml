<?xml version='1.0' encoding='utf-8'?>
<widget id="com.schtandart.dealer" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Schtandart Dealer App</name>
    <description>Schtandart Dealer Mobile Client</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Schtandart IT Division
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="www/img/icon_pre.PNG" />
        <icon density="mdpi" src="www/img/icon_pre.PNG" />
        <icon density="hdpi" src="www/img/icon_pre.PNG" />
        <icon density="xhdpi" src="www/img/icon_pre.PNG" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
